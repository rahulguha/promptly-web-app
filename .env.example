# Environment Variables Template
# Copy this file to .env and fill in your values

# Backend API Configuration
VITE_PUBLIC_BACKEND_API_BASE_URL=https://your-api-domain.com/v1

# Frontend URL for OAuth callbacks
VITE_PUBLIC_FRONTEND_URL=http://localhost:5175

# Development proxy target (for local development only)
VITE_API_PROXY_TARGET=https://your-api-domain.com

# Activity Tracking Types
VITE_ACTIVITY_TYPES=Logout,Profile_Created,Profile_Updated,Persona_Created,Persona_Updated,Template_Created,Template_Updated,Prompt_Created,Prompt_Executed_ChatGPT,Prompt_Executed_Claude,Prompt_Executed_Gemini,Prompt_Executed_Perplexity

# Node environment
NODE_ENV=development

# Application port
PORT=3000